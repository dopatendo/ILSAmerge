numres <- 4301
comp <- "Resumen de Comprobantes.xlsx"
rpt <- "rptentidades.xls"
comp <- as.data.frame(readxl::read_xlsx(comp))
rpt <- readxl::read_xls(rpt)
rpt <- na.omit(as.data.frame(rpt[,1:3]))[-1,]
colnames(rpt) <- c("codent","ruc","razon")
rpt <- rpt[-494,]
# VC ----------------------------------------------------------------------
vc <- cbind.data.frame(NA,NA,comp$`FECHA DE EMISIÓN`,
comp$CLIENTE,"038",NA,NA)
colnames(vc) <- c('Ase_cNummov','Per_cPeriodo',
'Ase_dFecha','Ase_cGlosa','Ase_cTipoMoneda',
'CreditoFiscal','MateriaConstruccion')
vc[,2] <- format(as.Date(vc$Ase_dFecha,format = "%d/%m/%Y"),"%m")
vc[,1] <- gsub(" ",0,format(numres:(numres+nrow(vc)-1),width = 10))
# VD ----------------------------------------------------------------------
i = 1
nms <- c('Ase_cNummov','Per_cPeriodo','Pla_cCuentaContable','Asd_nItem','Asd_cGlosa',
'Asd_nDebeSoles','Asd_nHaberSoles','Asd_nTipoCambio','Asd_nDebeMonExt',
'Asd_nHaberMonExt','Cos_cCodigo','Ten_cTipoEntidad','Ent_cCodEntidad',
'Asd_cTipoDoc','Asd_dFecDoc','Asd_cSerieDoc','Asd_cNumDoc','Asd_dFecVen',
'Asd_cBaseImp','Asd_cOperaTC','Asd_cTipoMoneda','Asd_cTipoDocRef',
'Asd_dFecDocRef','Asd_cSerieDocRef','Asd_cNumDocRef','Id_Exoneracion',
'Id_Tipo_Renta','Id_Modalidad','Id_Aduana','Id_Clasific_Servicio')
vd <- do.call(rbind.data.frame,lapply(1:nrow(vc),function(i){
if(comp$`RUC / DNI: CLIENTE`[i]=="00000000"){
rpti <- "00105"
}else{
rpti <- rpt[match(comp$`RUC / DNI: CLIENTE`[i],rpt$ruc),1]
}
vdi <- cbind.data.frame(rep(vc[i,1],4),
rep(vc[i,2],4),
c('12122',
'40111',
'70111',
'41161'),999,
comp$CLIENTE[i],
c(0,comp$IGV[i],comp$`BASE IMPONIBLE`[i],comp$`MONTO INAFECTO`[i]),
0,
0,
0,
NA,
c(rpti,NA,NA,NA),
999,
c("C",NA,NA,NA),
(vc$Ase_dFecha[i]),
comp$SERIE[i],
comp$CORRELATIVO[i],
NA,
c(NA,"002","002","024"),
"SCV",
"038",
c(round(comp[i,]$`VENTA TOTAL`,2),0,0,0),
NA,NA,NA,NA,NA,NA,NA,NA,NA)
colnames(vdi) <- nms
vdi
}))
vd$Asd_nItem <- 1:nrow(vd)
vd
seq(from = 1, to = nrow(vd))
seq(from = 1, to = nrow(vd),by = 4)
vd$Ent_cCodEntidad
vd
comp$`BASE IMPONIBLE`
vc
comp[1,]
comp$`VENTA TOTAL`
rm(list = ls())

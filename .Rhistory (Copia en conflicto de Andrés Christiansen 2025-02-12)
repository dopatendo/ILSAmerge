library(ILSAmerge)
input = "/Volumes/Sin título"
output = "/Users/andreschristiansen/Downloads/merged"
# i=5 iccs 2009
dirs <- list.files(input)
total <- 1:length(dirs)
total = 1:38
# notworking <- 19
#
# did <- total[total>max(notworking)]
# did <- setdiff(did,notworking)
# did = 18:20
i = 4
ptm <- proc.time()
for(i in total){
# ii <- ILSAfile.info(file.path(input,dirs[i],"Data"))
# cual <- ii$Population[2]
ILSAreadylocal(inputdir = file.path(input,dirs[i],"Data"),
outputdir = output,
# population = cual,
MBlistlimit = 300,
quiet = FALSE,filetype = "rds")
print(dirs[i])
print(i)
print(proc.time()-ptm)
}
